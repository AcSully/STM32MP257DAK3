&adc_3 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&adc3_pins_mx>;
	pinctrl-1 = <&adc3_sleep_pins_mx>;
	status = "okay";

	/* USER CODE BEGIN adc_3 */
	vdda-supply = <&vdda_1v8>;
	vref-supply = <&vddref_1v8>;

	adc3: adc@0 {
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		channel@2 {
			reg = <2>;               			/* ADC3 in2 channel is used */
			st,min-sample-time-ns = <10000>;  	/* 10¦Ìs sampling time */
		};
	};
	/* USER CODE END adc_3 */
};

&combophy {
